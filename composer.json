{
  "name": "hamza1886/shorturl",
  "description": "Bijective conversion between natural numbers (IDs) and short strings. Useful for URL shortener and short IDs in your permalinks.",
  "keywords": ["shortener", "urlshortener"],
  "homepage": "https://github.com/hamza1886",
  "support": {
    "issues": "https://github.com/hamza1886/issues",
    "source": "https://github.com/hamza1886"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Hamza Rashid",
      "email": "hamza1886@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "autoload": {
    "psr-4": {
      "ShortURL\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ShortURL\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}